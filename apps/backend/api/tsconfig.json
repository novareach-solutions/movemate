{
  "compilerOptions": {
    "declaration": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./out",
    "incremental": true,
    "strictNullChecks": false,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false
  },
  "include": ["src/**/*.ts", "ormconfig.ts"],
  "exclude": ["node_modules", "out"],
  "extends": "../../../tsconfig.json"
}
